{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/Navbar/*": ["components/Navbar/*"],
      "@/Cards/*": ["components/Cards/*"],
      "@/Profile/*": ["components/Profile/*"],
      "@/Tag/*": ["components/Tag/*"],
      "@/hooks/*": ["hooks/*"],
      "@/reducers/*": ["reducers/*"],
      "@/utils/*": ["utils/*"],
      "@/Persistence/*": ["Persistence/*"],
      "@/types/*": ["types/*"],
      "@/public/*": ["public/*"],
      "@/styles/*": ["styles/*"],
      "@/Icons/*": ["components/Icons/*"],
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "components/FormElement/index.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
